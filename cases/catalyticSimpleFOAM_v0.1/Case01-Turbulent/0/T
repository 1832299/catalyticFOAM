FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 573.15;

boundaryField
{
    catalyticWall
    {
        type            fixedValue;
        value           uniform 573.15;
    }
    fixedWall
    {
        type            zeroGradient;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 573.15;
    }
    outlet
    {
	type            inletOutlet;
        inletValue      uniform 573.15;
        value           uniform 573.15;
    }
}


